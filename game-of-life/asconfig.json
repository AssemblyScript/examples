{
  "options": {
    "runtime": "stub",
    "sourceMap": true,
    "measure": true
  },
  "targets": {
    "debug": {
      "outFile": "build/debug.wasm",
      "textFile": "build/debug.wat",
      "use": "Math=JSMath",
      "importMemory": true,
      "debug": true
    },
    "release": {
      "outFile": "build/release.wasm",
      "textFile": "build/release.wat",
      "use": "Math=JSMath",
      "importMemory": true,
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  }
}
