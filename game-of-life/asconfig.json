{
  "targets": {
    "options": {
      "runtime": "stub",
      "sourceMap": true,
      "measure": true
    },
    "debug": {
      "outFile": "build/untouched.wasm",
      "textFile": "build/untouched.wat",
      "use": "Math=JSMath",
      "importMemory": true,
      "debug": true
    },
    "release": {
      "outFile": "build/optimized.wasm",
      "textFile": "build/optimized.wat",
      "use": "Math=JSMath",
      "importMemory": true,
      "optimizeLevel": 3,
      "converge": false,
      "shrinkLevel": 0,
      "noAssert": false
    }
  }
}
